{
  "name": "tideline",
  "version": "1.4.0",
  "description": "Tidepool's timeline data visualization",
  "repository": {
    "type": "git",
    "url": "https://github.com/tidepool-org/tideline.git"
  },
  "publishConfig": {
    "registry": "https://nexus.ci.diabeloop.eu/repository/your-loops-npm/"
  },
  "main": "js/index.js",
  "license": "BSD-2-Clause",
  "scripts": {
    "clean": "./node_modules/.bin/rimraf ./dist/*",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production ./node_modules/.bin/webpack --optimize-minimize",
    "test": "TZ=UTC NODE_ENV=test ./node_modules/karma/bin/karma start",
    "browser-tests": "./node_modules/karma/bin/karma start --browsers Chrome",
    "test-watch": "TZ=UTC NODE_ENV=test ./node_modules/karma/bin/karma start --no-single-run --reporters=mocha",
    "start": "NODE_ENV=development webpack-dev-server -d --cache --colors --progress --content-base example --port 8081",
    "lint": "NODE_ENV=test ./node_modules/.bin/eslint js plugins example test"
  },
  "dependencies": {
    "bows": "1.7.0",
    "classnames": "2.2.6",
    "crossfilter2": "1.4.7",
    "d3": "3.5.17",
    "d3.chart": "0.3.0",
    "duration-js": "4.0.0",
    "i18next": "11.9.0",
    "lodash": "4.17.15",
    "moment-timezone": "0.5.26",
    "react": "15.6.2",
    "react-addons-pure-render-mixin": "15.6.2",
    "react-dom": "15.6.2",
    "react-sizeme": "2.5.2",
    "sundial": "mdblp/sundial#dblp.0.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "babel-loader": "8.0.6",
    "babel-eslint": "10.0.2",
    "babel-preset-react-app": "9.0.0",
    "babel-plugin-istanbul": "5.2.0",
    "babel-plugin-rewire": "^1.2.0",
    "chai": "4.2.0",
    "chromedriver": "76.0.0",
    "css-loader": "3.1.0",
    "postcss-loader": "3.0.0",
    "postcss-calc": "7.0.1",
    "postcss-custom-properties": "9.0.2",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-15": "1.4.0",
    "eslint": "6.1.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-lodash": "6.0.0",
    "eslint-plugin-moment-utc": "1.0.0",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "1.7.0",
    "file-loader": "4.1.0",
    "karma": "4.2.0",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-intl-shim": "1.0.3",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-sinon": "1.0.5",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "4.0.2",
    "less": "3.9.0",
    "less-loader": "5.0.0",
    "mocha": "6.2.0",
    "react-addons-test-utils": "15.6.2",
    "rewire": "2.5.2",
    "rewire-webpack": "1.0.1",
    "script-loader": "0.7.0",
